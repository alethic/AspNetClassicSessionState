<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net47</TargetFramework>
    <OutputType>Exe</OutputType>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <EnableCOMImport>true</EnableCOMImport>
    <EmbedCOMManifest>true</EmbedCOMManifest>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <LangVersion>7.2</LangVersion>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <LangVersion>7.2</LangVersion>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ApplicationHost.config" />
    <None Remove="Web.config" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="ApplicationHost.config" />
    <EmbeddedResource Include="Web.config" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Cogito.HostedWebCore" Version="1.1.0" />
    <PackageReference Include="FileAndServe.MSBuild.COM" Version="0.1.0-unstable0047" />
    <PackageReference Include="FileAndServe.MSBuild.WebImport" Version="0.1.0-unstable0047" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AspNetClassicSessionState.AspNet\AspNetClassicSessionState.AspNet.csproj">
      <Private>false</Private>
    </ProjectReference>
    <ProjectReference Include="..\AspNetClassicSessionState.Sample\AspNetClassicSessionState.Sample.csproj">
      <Private>false</Private>
    </ProjectReference>
  </ItemGroup>

</Project>
